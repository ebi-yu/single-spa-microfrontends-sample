{
  "rules": {
    "typescript": "error",
    "unicorn": "warn",
    "react": "off",
    "vue": "error",
    "jsx-a11y": "warn",
    "oxc": "error"
  },
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "globals": {
    "System": "readonly"
  },
  "plugins": ["vue", "typescript"],
  "settings": {
    "vue": {
      "version": "3"
    }
  }
}
